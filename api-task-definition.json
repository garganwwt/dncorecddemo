{
    "memory": "1024",
    "networkMode": "awsvpc",
    "cpu": "512",
    "family": "fargate-task-definition",
    "requiresCompatibilities": [
       "FARGATE"
    ],
    "containerDefinitions": [
       {
          "essential": true,
          "name": "dncoreapi",
          "image": "ghcr.io/garganwwt/dncorecicddemoghactions/dncoredb:latest",
          "repositoryCredentials": {
            "credentialsParameter": "arn:aws:secretsmanager:us-east-2:367444460268:secret:ag-github-aws-ZgvIID"
          },
          "environment" : [
            { "name" : "DB_CONN_STR", "value" : "host=admindb;port=5432;database=qsr-onboarding-admin;user id=postgres;password=postgres" }
          ],
          "portMappings": [ 
            { 
               "containerPort": 5001
            }
         ],
         "logConfiguration": {
             "logDriver": "awslogs",
             "options": {
                 "awslogs-group": "awslogs-my-cd-demo",
                 "awslogs-region": "us-west-2",
                 "awslogs-stream-prefix": "cd-demo"
             }
         },
          "cpu": 512,
          "memoryReservation": 1024
       }
    ],
    "executionRoleArn": "arn:aws:iam::367444460268:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::367444460268:role/ecsTaskExecutionRole"
 }
