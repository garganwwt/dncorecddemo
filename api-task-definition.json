{
    "memory": "1024",
    "networkMode": "awsvpc",
    "cpu": "512",
    "family": "fargate-task-definition",
    "requiresCompatibilities": [
       "FARGATE"
    ],
    "containerDefinitions": [
       {
          "essential": true,
          "name": "dncoreapi",
          "image": "docker.pkg.github.com/srirangv/dncorecicddemoghactions/dncoredb:latest",
          "repositoryCredentials": {
            "credentialsParameter": "arn:aws:secretsmanager:us-east-2:367444460268:secret:githubregistrytoken-9c0pcz"
          },
          "environment" : [
            { "name" : "DB_CONN_STR", "value" : "${{ secrets.DB_CONN_STR }}" }
          ],
          "cpu": 512,
          "memoryReservation": 1024
       }
    ],
    "executionRoleArn": "arn:aws:iam::367444460268:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::367444460268:role/ecsTaskExecutionRole"
 }